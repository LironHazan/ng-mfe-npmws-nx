{
  "version": 2,
  "projects": {
    "ft1_app": {
      "projectType": "application",
      "root": "packages/ft1_app",
      "sourceRoot": "packages/ft1_app/src",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/ft1_app",
            "index": "packages/ft1_app/src/index.html",
            "main": "packages/ft1_app/src/main.ts",
            "polyfills": "packages/ft1_app/src/polyfills.ts",
            "tsConfig": "packages/ft1_app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/ft1_app/src/favicon.ico",
              "packages/ft1_app/src/assets"
            ],
            "styles": [
              "packages/ft1_app/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "packages/ft1_app/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/ft1_app/src/environments/environment.ts",
                  "with": "packages/ft1_app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "packages/ft1_app/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "ft1_app:build:production"
            },
            "development": {
              "browserTarget": "ft1_app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ft1_app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/ft1_app/src/**/*.ts",
              "packages/ft1_app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/ft1_app"
          ],
          "options": {
            "jestConfig": "packages/ft1_app/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "reactapp": {
      "root": "packages/reactapp",
      "sourceRoot": "packages/reactapp/src",
      "projectType": "application",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "outputPath": "dist/packages/reactapp",
            "index": "packages/reactapp/src/index.html",
            "main": "packages/reactapp/src/main.tsx",
            "polyfills": "packages/reactapp/src/polyfills.ts",
            "tsConfig": "packages/reactapp/tsconfig.app.json",
            "assets": [
              "packages/reactapp/src/favicon.ico",
              "packages/reactapp/src/assets"
            ],
            "styles": [
              "packages/reactapp/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "packages/reactapp/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/reactapp/src/environments/environment.ts",
                  "with": "packages/reactapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "reactapp:build",
            "hmr": true,
            "port": 4204
          },
          "configurations": {
            "production": {
              "buildTarget": "reactapp:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/reactapp/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/reactapp"
          ],
          "options": {
            "jestConfig": "packages/reactapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "shared-data-access": {
      "projectType": "library",
      "root": "packages/shared/data-access",
      "sourceRoot": "packages/shared/data-access/src",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/shared/data-access"
          ],
          "options": {
            "jestConfig": "packages/shared/data-access/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/shared/data-access/src/**/*.ts",
              "packages/shared/data-access/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shell": {
      "projectType": "application",
      "root": "packages/shell",
      "sourceRoot": "packages/shell/src",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/shell",
            "index": "packages/shell/src/index.html",
            "main": "packages/shell/src/main.ts",
            "polyfills": "packages/shell/src/polyfills.ts",
            "tsConfig": "packages/shell/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/shell/src/favicon.ico",
              "packages/shell/src/assets"
            ],
            "styles": [
              "packages/shell/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "packages/shell/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/shell/src/environments/environment.ts",
                  "with": "packages/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "packages/shell/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {}
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/shell/src/**/*.ts",
              "packages/shell/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/shell"
          ],
          "options": {
            "jestConfig": "packages/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "serve-mfe": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx serve ft1_app",
              "nx serve shell",
              "nx serve reactapp"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
