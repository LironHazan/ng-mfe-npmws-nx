{
  "version": 2,
  "projects": {
    "ft1-app": {
      "projectType": "application",
      "root": "packages/ft1-app",
      "sourceRoot": "packages/ft1-app/src",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/ft1-app",
            "index": "packages/ft1-app/src/index.html",
            "main": "packages/ft1-app/src/main.ts",
            "polyfills": "packages/ft1-app/src/polyfills.ts",
            "tsConfig": "packages/ft1-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/ft1-app/src/favicon.ico",
              "packages/ft1-app/src/assets"
            ],
            "styles": [
              "packages/ft1-app/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "packages/ft1-app/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/ft1-app/src/environments/environment.ts",
                  "with": "packages/ft1-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "packages/ft1-app/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "ft1-app:build:production"
            },
            "development": {
              "browserTarget": "ft1-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ft1-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/ft1-app/src/**/*.ts",
              "packages/ft1-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/ft1-app"
          ],
          "options": {
            "jestConfig": "packages/ft1-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "shell": {
      "projectType": "application",
      "root": "packages/shell",
      "sourceRoot": "packages/shell/src",
      "prefix": "my-workspaces-playground-project",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/shell",
            "index": "packages/shell/src/index.html",
            "main": "packages/shell/src/main.ts",
            "polyfills": "packages/shell/src/polyfills.ts",
            "tsConfig": "packages/shell/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "packages/shell/src/favicon.ico",
              "packages/shell/src/assets"
            ],
            "styles": [
              "packages/shell/src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "packages/shell/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "packages/shell/src/environments/environment.ts",
                  "with": "packages/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "packages/shell/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {}
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/shell/src/**/*.ts",
              "packages/shell/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/shell"
          ],
          "options": {
            "jestConfig": "packages/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "serve-mfe": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx serve ft1-app",
              "nx serve shell"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
